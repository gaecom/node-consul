  "main": "lib/consul.js",
  "dependencies": {
    "@sealsystems/droddel": "2.0.0",
    "@sealsystems/tlscert": "2.1.2",
    "app-root-path": "2.1.0",
    "async-retry": "1.2.3",
    "consul": "0.34.1",
    "dnscache": "1.0.1",
    "eventemitter2": "5.0.1",
    "getenv": "0.7.0",
    "lodash.flatten": "4.4.0",
    "parse-duration": "0.1.1"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "1.1.1",
    "assertthat": "2.0.0",
    "docker-host": "3.1.0",
    "nodeenv": "1.0.0",
    "nyc": "13.1.0",
    "proxyquire": "2.1.0",
    "roboter": "3.0.3",
    "uuidv4": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-consul.git"
