    "report": "npm run coverage && if [[ $OSTYPE == darwin* ]]; then open=open; else open=xdg-open; fi && $open coverage/lcov-report/index.html"
  },
  "main": "lib/consul.js",
  "dependencies": {
    "app-root-path": "2.0.1",
    "async": "2.5.0",
    "consul": "0.29.0",
    "dnscache": "1.0.1",
    "getenv": "0.7.0",
    "lodash": "4.17.4",
    "parse-duration": "0.1.1",
    "retry": "0.10.1",
    "seal-droddel": "1.0.1",
    "seal-log": "1.2.0",
    "seal-tlscert": "1.2.1"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "1.1.1",
    "assertthat": "0.10.3",
    "docker-host": "3.1.0",
    "eslint-config-seal": "0.0.9",
    "nodeenv": "1.0.0",
    "nyc": "13.0.1",
    "proxyquire": "1.8.0",
    "roboter": "0.16.0",
    "roboter-server": "0.16.0",
    "uuidv4": "0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-consul.git"
  },
  "release": {
    "extends": "@sealsystems/semantic-release-config/node-module"
