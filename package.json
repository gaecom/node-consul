{
  "name": "@sealsystems/seal-consul",
  "version": "3.5.13",
  "description": "seal-consul provides service discovery based on Consul.",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "scripts": {
    "bot": "bot",
    "release": "semantic-release --branch ${CIRCLE_BRANCH}",
    "test": "bot test-units",
